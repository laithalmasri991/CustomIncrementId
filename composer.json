{
    "name": "laith/custom-increment-id",
    "description": "A Magento 2 module to customize increment IDs for invoices and credit memos.",
    "type": "magento2-module",
    "version": "1.1.2",
    "license": "MIT",
    "authors": [
        {
            "name": "Laith Almasri",
            "email": "laith.k.m4@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "magento/framework": "^103.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "suggest": {
        "magento/module-page-cache": "Provides full-page caching functionality"
    },
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Laith\\CustomIncrementId\\": ""
        }
    },
    "extra": {
        "map": [
            [
                "*",
                "Laith/CustomIncrementId"
            ]
        ]
    }
}

